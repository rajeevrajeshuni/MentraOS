{
  "name": "@augmentos/cloud-client",
  "version": "0.1.0",
  "description": "Pure TypeScript SDK for connecting to and interacting with the AugmentOS cloud platform",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./testing": {
      "import": "./dist/testing/index.mjs",
      "require": "./dist/testing/index.js",
      "types": "./dist/testing/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "bun run build:types && bun run build:esm && bun run build:cjs",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationMap",
    "build:esm": "bun build src/index.ts --outdir dist --format esm --external ws --external node:* --external livekit-client --external @livekit/rtc-node --external wrtc --target node",
    "build:cjs": "bun build src/index.ts --outdir dist --format cjs --external ws --external node:* --external livekit-client --external @livekit/rtc-node --external wrtc --target node",
    "dev": "bun run src/examples/basic-test.ts",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:accounts": "bun run src/examples/account-test.ts",
    "test:connection": "bun run src/examples/connection-test.ts",
    "test:azure-stress": "bun run src/examples/azure-rate-limit-stress-test.ts",
    "test:azure-quick": "bun run src/examples/azure-rate-limit-quick-test.ts",
    "test:live-captions": "bun run src/examples/live-captions-test.ts",
    "test:mira": "bun run src/examples/mira-test.ts",
    "test:translation": "bun run src/examples/translation-test.ts",
    "test:livekit": "bun run src/examples/livekit-handshake-test.ts",
    "test:livekit-sub": "bun run src/examples/livekit-subscriber-test.ts",
    "test:livekit-node-pub": "bun run src/examples/livekit-node-publisher-test.ts",
    "test:rtc-node-pub": "bun run src/examples/rtc-node-publisher.ts",
    "test:livekit-diagnose": "bun run src/examples/livekit-pubsub-diagnose.ts",
    "monitor:live-captions": "bun run src/examples/live-captions-monitor.ts",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "augmentos",
    "smart-glasses",
    "sdk",
    "typescript",
    "websocket",
    "testing",
    "client"
  ],
  "author": "AugmentOS Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AugmentOS-Community/AugmentOS.git",
  "directory": "cloud/packages/cloud-client"
  },
  "dependencies": {
    "ws": "^8.18.0",
    "events": "^3.3.0",
    "jsonwebtoken": "^9.0.2",
    "express": "^4.19.2",
    "livekit-client": "^2.5.7",
    "@livekit/rtc-node": "^0.13.18",
    "wrtc": "npm:@roamhq/wrtc@^0.8.0"
  },
  "devDependencies": {
    "@types/ws": "^8.5.12",
    "@types/node": "^20.12.0",
    "@types/jsonwebtoken": "^9.0.6",
    "typescript": "^5.2.0",
    "eslint": "^9.20.1",
    "@eslint/js": "^9.20.0",
    "typescript-eslint": "^8.24.0",
    "bun-types": "^1.0.17",
    "@types/express": "^4.17.21"
  },
  "peerDependencies": {
    "typescript": ">=4.7.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "bun": ">=1.0.0"
  }
}