{
  "name": "@mentra/cloud",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "mkdir -p dist && bun x tsc -p tsconfig.json",
    "postbuild": "cp -r src/public dist/public && cp src/services/lc3/liblc3.wasm dist/services/lc3/liblc3.wasm",
    "start": "bun dist/index.js",
    "dev": "bun --watch src/index.ts",
    "staging": "NODE_ENV=staging tsx watch src/index.ts",
    "deploy": "bun run build && bun run start",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "test": "bun run ./src/tests/run-tests.ts",
    "test:visual": "bun run ./src/services/layout/tests/run-visual-tests.ts",
    "test:display": "bun run ./src/services/layout/tests/run-visual-tests.ts",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org pierce-50 --project augmentos-cloud-dev ./dist && sentry-cli sourcemaps upload --org pierce-50 --project augmentos-cloud-dev ./dist",
    "use-linked": "bun link @mentra/utils @mentra/sdk @mentra/agents",
    "migrate:permissions": "bun run src/scripts/migrate-permissions.ts",
    "verify:permissions": "bun run src/scripts/verify-permissions.ts"
  },
  "author": "Mentra",
  "devDependencies": {
    "@types/babel__core": "^7.20.5",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/express-serve-static-core": "^4.17.43",
    "@types/form-data": "^2.5.0",
    "@types/helmet": "^4.0.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.6.1",
    "@types/stream-buffers": "^3.0.4",
    "@types/ws": "^8.5.14",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "chalk": "^5.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.0",
    "husky": "^7.0.1",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@livekit/rtc-node": "^0.13.18",
    "@logtail/pino": "^0.5.4",
    "@mentra/sdk": "workspace:*",
    "@sentry/bun": "^9.1.0",
    "@sentry/cli": "^2.42.1",
    "@supabase/supabase-js": "^2.49.1",
    "axios": "^1.7.4",
    "bun-types": "1.0.17",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.18.2",
    "form-data": "^4.0.1",
    "helmet": "^4.6.0",
    "jose": "^6.0.11",
    "jsonwebtoken": "^9.0.1",
    "livekit-server-sdk": "^2.13.2",
    "microsoft-cognitiveservices-speech-sdk": "^1.44.1",
    "moment": "^2.29.4",
    "mongodb": "^6.13.0",
    "mongoose": "^6.5.2",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.28.0",
    "pino": "^9.6.0",
    "pino-http": "^10.5.0",
    "pino-pretty": "^13.0.0",
    "posthog-node": "^4.14.0",
    "resend": "^4.5.1",
    "uuid": "^11.0.5",
    "ws": "^8.18.1"
  }
}
